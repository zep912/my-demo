<view class="container"><view class="list-cell b-b m-t" hover-class="cell-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['navTo',['头像']]]]]}}" bindtap="__e"><text class="cell-tit">头像</text><image class="cell-tip-img _img" src="{{person.avartar}}"></image><text class="cell-more yticon icon-you cell-more-middle"></text></view><view class="list-cell b-b" hover-class="cell-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['navTo',['用户名']]]]]}}" bindtap="__e"><text class="cell-tit">用户名</text><text class="cell-tip">{{person.userName}}</text><text class="cell-more yticon icon-you"></text></view><view class="list-cell b-b" hover-class="cell-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['navTo',['性别']]]]]}}" bindtap="__e"><text class="cell-tit">性别</text><text class="cell-tip">{{person.sex}}</text><text class="cell-more yticon icon-you"></text></view><view class="list-cell b-b" hover-class="cell-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['navTo',['生日']]]]]}}" bindtap="__e"><text class="cell-tit">生日</text><text class="cell-tip">{{person.birthday}}</text><text class="cell-more yticon icon-you"></text></view><view class="list-cell b-b" hover-class="cell-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['navTo',['体重']]]]]}}" bindtap="__e"><text class="cell-tit">体重</text><text class="cell-tip">{{person.weight}}</text><text class="cell-more yticon icon-you"></text></view><view class="list-cell log-out-btn log-save" id="log-save" data-event-opts="{{[['tap',[['save',['$event']]]]]}}" bindtap="__e"><text class="cell-tit">保存</text></view></view>