page {
  background: #fff;
}
.divider {
  width: 100%;
  height: 10rpx;
  background: #f2f2f2;
}
.cancelBox {
  width: 100%;
  height: 320rpx;
  background: white;
  border-top: 1px solid #E3E3E3;
  text-align: center;
  padding: 50rpx 110rpx;
  margin-bottom: 54rpx;
}
.cancelBox ._img {
    width: 77rpx;
    height: 77rpx;
    margin-bottom: 25rpx;
}
.cancelBox text {
    display: block;
}
.cancelBox .cancel-success {
    font-size: 32rpx;
    font-weight: bold;
    color: #545454;
    margin-bottom: 28rpx;
}
.cancelBox .cancel-than {
    font-size: 24rpx;
    color: #545454;
    line-height: 36rpx;
}
.cancelReason {
  margin-top: 10rpx;
  background: #fff;
}
.cancelReason .cancelReason-top {
    width: 100%;
    height: 90rpx;
    border-bottom: 1px solid #E3E3E3;
    padding-left: 30rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
    font-size: 28rpx;
    font-weight: bold;
    color: #545454;
}
.cancelReason .cancelReason-list {
    padding: 34rpx 30rpx 98rpx 60rpx;
}
.cancelReason .cancelReason-list .van-radio {
      font-size: 26rpx;
      font-weight: 500;
      color: #454545;
      line-height: 40rpx;
      margin-bottom: 65rpx;
}
.cancelReason .cancelReason-list .textarea {
      width: 95%;
      height: 242rpx;
      background: #f2f2f2;
      border-radius: 20rpx;
      margin-left: 28rpx;
      -webkit-box-sizing: border-box;
              box-sizing: border-box;
      padding-top: 24rpx;
      padding-right: 32rpx;
      padding-left: 32rpx;
}
.cancelReason .textareaPlace {
    font-size: 24rpx;
    font-weight: 500;
    color: #949494;
}
.btn {
  width: 94%;
  margin: 0 auto;
  height: 90rpx;
  background: #f7b52c;
  border-radius: 45rpx;
