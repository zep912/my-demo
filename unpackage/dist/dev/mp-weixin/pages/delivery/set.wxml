<view class="container"><view class="user-info-box"><image class="portrait _img" src="{{img}}"></image><view class="info-box"><text class="username"><text class="name">{{form.userName}}</text><text class="username-info">{{form.phone}}</text></text></view></view><view class="list-cell b-b m-t" hover-class="cell-hover" hover-stay-time="{{50}}"><text class="cell-tit">我的花名</text><text class="cell-tip">{{form.userName}}</text></view><view class="list-cell b-b" style="margin-top:10rpx;" hover-class="cell-hover" hover-stay-time="{{50}}"><text class="cell-tit">接单状态</text><van-switch vue-id="1" checked="{{checked}}" active-color="#07c160" custom-class="switchClass" data-event-opts="{{[['^change',[['onChange']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l"></van-switch></view><view class="list-cell" hover-class="cell-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['navTo',['statistics']]]]]}}" bindtap="__e"><text class="cell-tit">送单统计</text><text class="cell-more yticon icon-you"></text></view><view class="list-cell" style="margin-top:10rpx;" hover-class="cell-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['navTo',['myprofit']]]]]}}" bindtap="__e"><text class="cell-tit">我的收益</text><text class="cell-more yticon icon-you"></text></view><view class="de-foot"><view data-event-opts="{{[['tap',[['sd',[1]]]]]}}" class="{{['de-songdan',(daliveryData===1)?'active':'']}}" bindtap="__e"><image src="{{songdan}}" alt class="_img"></image><text>全部送单</text></view><view data-event-opts="{{[['tap',[['sd',[2]]]]]}}" class="{{['de-jiedanset',(daliveryData===2)?'active':'']}}" bindtap="__e"><image src="{{jiedan}}" alt class="_img"></image><text>接单设置</text></view></view></view>