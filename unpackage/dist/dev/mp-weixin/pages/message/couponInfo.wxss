.container {
  padding: 20rpx;
  height: 100vh;
  background: #F2F2F2;
}
.container .content .top-time {
    font-size: 24rpx;
    color: #9F9F9F;
    text-align: center;
    line-height: 60rpx;
}
.container .content .item {
    padding: 20rpx 30rpx;
    background: #fff;
    font-size: 28rpx;
    border-radius: 20rpx;
    line-height: 40rpx;
}
.container .content .item .center {
      font-size: 24rpx;
      color: #787878;
      height: 90rpx;
}
.container .content .item .bottom {
      border-top: 2rpx solid #f2f2f2;
      font-size: 20rpx;
      color: #B0B0B0;
      padding-top: 20rpx;
}
.container .content .item .bottom .icon-gengduo {
        float: right;
}
.container .null-class {
    color: #C9C9C9;
    text-align: center;
    margin-top: 40%;
}
.container .null-class .iconfont {
      font-size: 140rpx;
      margin-bottom: 80rpx;
}

