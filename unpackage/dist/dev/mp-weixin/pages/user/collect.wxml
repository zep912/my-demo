<view class="collect"><block wx:if="{{collectGoods.length==0}}"><empty vue-id="1" src="{{src}}" msg="{{msg}}" bind:__l="__l"></empty></block><block wx:if="{{collectGoods.length!=0}}"><view><view class="collect-wrap"><text>您收藏了<text class="collect-num">10</text>件商品</text><text data-event-opts="{{[['tap',[['editClick',['$event']]]]]}}" bindtap="__e">{{edit}}</text></view><view class="collect-goods"><block wx:for="{{collectGoods}}" wx:for-item="item" wx:for-index="index"><view class="collect-goods-list"><block wx:if="{{isShow}}"><view class="collect-vanCheck"><van-checkbox vue-id="{{'2-'+index}}" value="{{checked}}" checked-color="#F7B52C" name="{{index}}" data-event-opts="{{[['^change',[['onChange']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l"></van-checkbox></view></block><image src="{{item.img}}" alt class="_img"></image><view class="collectGoodsContent"><view class="collect-attr"><text class="ziyin">自营</text><text class="title">{{item.title}}</text><text class="attr">{{item.attr}}</text></view><view class="collect-price"><text class="price">{{item.price}}</text><text class="name">{{item.name}}</text></view></view><view class="icon"><van-icon vue-id="{{'3-'+index}}" name="add" color="#F55641" size="30px" data-com-type="wx" bind:__l="__l"></van-icon></view></view></block></view><block wx:if="{{isShow}}"><view class="foot"><van-checkbox vue-id="4" value="{{checked}}" checked-color="#F7B52C" icon-size="26" label-class="labelClass" data-event-opts="{{[['^change',[['allOnChange']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}">全选</van-checkbox><view data-event-opts="{{[['tap',[['dele',['$event']]]]]}}" class="dele-collect" bindtap="__e">删除收藏</view></view></block></view></block></view>