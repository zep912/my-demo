.wrap {
  width: 680rpx;
  margin: 30rpx auto;
}
.coupon {
  display: inline-block;
  border-radius: 10rpx;
  position: relative;
}
.coupon-left {
  float: left;
  width: 200rpx;
  height: 200rpx;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  padding-top: 48rpx;
  padding-bottom: 48rpx;
}
.coupon-left::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  height: 50%;
  background: -o-radial-gradient(right top, circle, transparent 10px, #F5F1DD 10px, #EAE5C7 10rpx);
  background: radial-gradient(circle at right top, transparent 10px, #F5F1DD 10px, #EAE5C7 10rpx);
}
.coupon-left::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 50%;
  background: -o-radial-gradient(right bottom, circle, transparent 10px, #F5F1DD 10px, #EAE5C7 10rpx);
  background: radial-gradient(circle at right bottom, transparent 10px, #F5F1DD 10px, #EAE5C7 10rpx);
}
.coupon-con {
  float: left;
  width: 480rpx;
  height: 200rpx;
  position: relative;
  padding-top: 32rpx;
  padding-bottom: 26rpx;
  padding-left: 32rpx;
}
.coupon-con::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  height: 50%;
  background: -o-radial-gradient(left top, circle, transparent 10px, #F5F1DD 10px, #EAE5C7 10rpx);
  background: radial-gradient(circle at left top, transparent 10px, #F5F1DD 10px, #EAE5C7 10rpx);
}
.coupon-con::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 50%;
  background: -o-radial-gradient(left bottom, circle, transparent 10px, #F5F1DD 10px, #EAE5C7 10rpx);
  background: radial-gradient(circle at left bottom, transparent 10px, #F5F1DD 10px, #EAE5C7 10rpx);
}
.tab2 .coupon-left::before, .tab3 .coupon-left::before {
  background: -o-radial-gradient(right top, circle, transparent 10px, #EDEDED 10px, #EDEDED 10rpx);
  background: radial-gradient(circle at right top, transparent 10px, #EDEDED 10px, #EDEDED 10rpx);
}
.tab2 .coupon-left::after, .tab3 .coupon-left::after {
  background: -o-radial-gradient(right bottom, circle, transparent 10px, #EDEDED 10px, #EDEDED 10rpx);
  background: radial-gradient(circle at right bottom, transparent 10px, #EDEDED 10px, #EDEDED 10rpx);
}
.tab2 .coupon-con::before, .tab3 .coupon-con::before {
  background: -o-radial-gradient(left top, circle, transparent 10px, #EDEDED 10px, #EDEDED 10rpx);
  background: radial-gradient(circle at left top, transparent 10px, #EDEDED 10px, #EDEDED 10rpx);
}
.tab2 .coupon-con::after, .tab3 .coupon-con::after {
  background: -o-radial-gradient(left bottom, circle, transparent 10px, #EDEDED 10px, #EDEDED 10rpx);
  background: radial-gradient(circle at left bottom, transparent 10px, #EDEDED 10px, #EDEDED 10rpx);
}
.coupon-left-box {
  height: auto;
  width: 100%;
  position: absolute;
  border-right: 1px dashed #D0B07A;
  z-index: 200;
}
.coupon-left-box ._img {
    position: absolute;
    left: 16%;
    top: 3%;
    color: #b98d5d;
    width: 20rpx;
    height: 20rpx;
}
.coupon-left-box .coupon-left-boxPrice {
    font-size: 80rpx;
    font-weight: 800;
    color: #b98d5d;
    display: block;
    text-align: center;
}
.coupon-left-box .coupon-left-boxman {
    display: block;
    font-size: 24rpx;
    font-weight: bold;
    color: #b98d5d;
    text-align: center;
}
.coupon-con-title {
  position: absolute;
}
.coupon-con-title text {
    display: block;
    position: relative;
    z-index: 200;
}
.coupon-con-title .coupon-con-titles {
    font-size: 26rpx;
    font-weight: bold;
    color: #b98d5d;
    margin-bottom: 30rpx;
}
.coupon-con-title .coupon-con-goods {
    font-size: 22rpx;
    font-weight: 500;
    color: #b98d5d;
    margin-bottom: 8rpx;
}
.coupon-con-title .coupon-con-time {
    font-size: 20rpx;
    font-weight: 400;
    color: #b98d5d;
}
.coupon-btn {
  position: absolute;
  width: 158rpx;
  height: 37rpx;
  background: #bd9968;
  right: 16rpx;
  bottom: 28rpx;
  font-size: 20rpx;
  font-weight: bold;
  color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  z-index: 200;
}
.couponImg {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  right: 5%;
  bottom: 5%;
  z-index: 200;
}
.tabwrap .coupon .coupon-left-boxPrice, .tabwrap .coupon .coupon-left-boxman {
  color: #BBBBBB;
}
.tabwrap .coupon .coupon-left-box {
  border-right: 1px dashed #BBBBBB;
}
.tabwrap .coupon .coupon-con .coupon-con-titles, .tabwrap .coupon .coupon-con .coupon-con-goods, .tabwrap .coupon .coupon-con .coupon-con-time {
  color: #BBBBBB;
}

