<view class="content"><view class="navbar"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['tabClick',[index]]]]]}}" class="{{['nav-item',(tabCurrentIndex===index)?'current':'']}}" bindtap="__e">{{''+item.text+''}}</view></block></view><swiper class="swiper-box" current="{{tabCurrentIndex}}" duration="300" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><block wx:for="{{navList}}" wx:for-item="tabItem" wx:for-index="tabIndex" wx:key="tabIndex"><swiper-item class="tab-content"><scroll-view class="list-scroll-content" scroll-y><block wx:if="{{deliverylist.length===0}}"><view class="delivery-empty"><image src="{{$root.m0}}" alt class="_img"></image><view>啥都没有~</view></view></block><view class="tab-tips"><text>提示:现在是收单模式</text></view><block wx:for="{{deliverylist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="order-item"><view class="delivery-list-tab"><view class="tab-time"><view>送达:<text>今天</text><text>{{''+item.time}}</text></view><view class="tab-time-countDown"><image class="tab-time-img" src="../../static/delivery/shalou.png" alt></image><van-count-down vue-id="{{'1-'+tabIndex+'-'+index}}" time="{{item.countDown}}" format="DD天HH:mm:ss" data-com-type="wx" bind:__l="__l"></van-count-down></view></view><view class="tab-step"><view class="tab-step-top"><text class="tips">{{'#'+item.info.a}}</text><text class="tips">{{'#'+item.info.b}}</text><text class="tips">{{'#'+item.info.c}}</text></view><van-steps vue-id="{{'2-'+tabIndex+'-'+index}}" steps="{{item.steps}}" active="{{active}}" direction="vertical" active-color="#F7B52C" custom-class="tab-van-step" data-com-type="wx" bind:__l="__l"></van-steps></view><block wx:if="{{tabInfoShow}}"><view class="tab-info"><text class="block">内容:<text>{{item.msg.content}}</text></text><text class="block">单号:<text>{{item.msg.oddNum}}</text></text><text class="block">来源:<text>{{item.msg.sorce}}</text></text><text class="block">备注:<text>{{item.msg.beizhu}}</text></text><text class="block">标识:<text>{{item.msg.biaoshi}}</text></text><text class="block">下单:<text>{{item.msg.time}}</text></text></view></block><view class="tab-ret"><view data-event-opts="{{[['tap',[['look',['$event']]]]]}}" bindtap="__e"><text>{{tabLook}}</text><van-icon vue-id="{{'3-'+tabIndex+'-'+index}}" name="{{iconName}}" data-com-type="wx" bind:__l="__l"></van-icon></view><button class="tab-btn">去取单</button></view></view></view></block></scroll-view></swiper-item></block></swiper><view class="de-foot"><view data-event-opts="{{[['tap',[['sd',[1]]]]]}}" class="{{['de-songdan',(daliveryData===1)?'active':'']}}" bindtap="__e"><image src="{{songdan}}" alt class="_img"></image><text>全部送单</text></view><view data-event-opts="{{[['tap',[['sd',[2]]]]]}}" class="{{['de-jiedanset',(daliveryData===2)?'active':'']}}" bindtap="__e"><image src="{{jiedan}}" alt class="_img"></image><text>接单设置</text></view></view></view>